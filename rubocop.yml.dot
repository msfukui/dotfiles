inherit_from: .rubocop_todo.yml

# Use only ascii symbols in comments.
Style/AsciiComments:
  Enabled: false

# Line Length
Layout/LineLength:
  Max: 120

# Use UTF-8 as the source file encoding.
Style/Encoding:
  Enabled: false

# Document classes and non-namespace modules.
Style/Documentation:
  Enabled: false
